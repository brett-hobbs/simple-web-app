{
  "name": "test-webapp",
  "version": "1.0.0",
  "description": "Simple React Web App",
  "main": "index.js",
  "scripts": {
    "test": "mocha --opts mocha.opts",
    "lint": "./node_modules/.bin/eslint ./src",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./",
    "prod": "NODE_ENV=production node ./src/server.js",
    "postinstall": "webpack -p",
    "watch:test": "mocha --watch --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brett-hobbs/test-webapp.git"
  },
  "author": "Brett Hobbs",
  "homepage": "https://github.com/brett-hobbs/test-webapp#readme",
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "jsdom": "^9.10.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "sinon": "^1.17.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
